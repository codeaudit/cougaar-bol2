      <class>
        org.cougaar.bootstrap.Bootstrapper
      </class>
      <vm_parameter>
        -Dorg.cougaar.node.name=BOLNode
      </vm_parameter>
      <vm_parameter>
        -Dorg.cougaar.system.path=COUGAAR_INSTALL_PATH/sys
      </vm_parameter>
      <vm_parameter>
        -Dorg.cougaar.core.node.InitializationComponent=XML
      </vm_parameter>
      <vm_parameter>
        -Dorg.cougaar.lib.web.scanRange=100
      </vm_parameter>
      <vm_parameter>
        -Dorg.cougaar.core.servlet.enable=true
      </vm_parameter>
      <vm_parameter>
        -Dorg.cougaar.lib.web.https.port=-1
      </vm_parameter>
      <vm_parameter>
        -Dorg.cougaar.config.path=.;COUGAAR_INSTALL_PATH/configs/common;COUGAAR_INSTALL_PATH/configs/glmtrans
      </vm_parameter>
      <vm_parameter>
        -Dorg.cougaar.lib.web.http.port=8800
      </vm_parameter>
      <vm_parameter>
        -Xms=-Xms64m
      </vm_parameter>
      <vm_parameter>
        -Xbootclasspath/p=-Xbootclasspath/p:COUGAAR_INSTALL_PATH/lib/javaiopatch.jar
      </vm_parameter>
      <vm_parameter>
        -Dorg.cougaar.core.agent.startTime=08/10/2005
      </vm_parameter>
      <vm_parameter>
        -Xoss=-Xoss256k
      </vm_parameter>
      <vm_parameter>
        -Dorg.cougaar.workspace=COUGAAR_INSTALL_PATH/workspace
      </vm_parameter>
      <vm_parameter>
        -Xmx=-Xmx768m
      </vm_parameter>
      <vm_parameter>
        -Duser.timezone=GMT
      </vm_parameter>
      <vm_parameter>
        -Dorg.cougaar.install.path=COUGAAR_INSTALL_PATH
      </vm_parameter>
      <vm_parameter>
        -Dorg.cougaar.core.logging.config.filename=log.properties
      </vm_parameter>
      <vm_parameter>
        -classpath=COUGAAR_INSTALL_PATH/lib/bootstrap.jar
